{
  "name": "movie-api",
  "version": "1.0.0",
  "description": "Movie DB REST API",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc -b",
    "api": "nodemon dist/server.js",
    "dev:api": "set NODE_ENV=dev&& concurrently \"npm run build\" \"npm run api\"",
    "test:api": "set NODE_ENV=test&& concurrently \"npm run build\" \"npm run api\"",
    "db:init": "npm run db:create && db:migrate:all && npm run db:seed:undo && npm run db:seed:all",
    "db:create": "sequelize db:create --env=development",
    "db:migrate": "sequelize db:migrate --env=development",
    "db:migrate:all": "sequelize db:migrate --env=development",
    "db:migrate:undo": "sequelize db:migrate:undo:all --env=development",
    "movie:generator": "npm run build && node ./dist/scripts/movie-factory.js",
    "db:seed:all": "sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo:all",
    "test": "set NODE_ENV=test&& mocha --require ts-node/register src/tests/**/*.ts",
    "prettier": "prettier --config .prettierrc src/**/*.ts --write",
    "lint": "eslint . --ext .ts",
    "lint-and-fix": "eslint . --ext .ts --fix"
  },
  "author": "Osvaldo Tejada",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ioredis": "^4.17.3",
    "lodash": "^4.17.20",
    "mysql2": "^2.1.0",
    "path": "^0.12.7",
    "sequelize": "^6.3.4",
    "sequelize-cli": "^6.2.0",
    "typescript": "^4.0.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.12",
    "@types/express": "^4.17.7",
    "@types/ioredis": "^4.17.3",
    "@types/lodash": "^4.14.159",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "lorem-ipsum": "^2.0.3",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.0",
    "ts-node": "^9.0.0"
  }
}
